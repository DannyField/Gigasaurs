<%# Renders the nav bar from the shared folder - know as a partial %>
<%= render 'shared/navbar' %>

<div class="user-container">

<h1>Your Cart</h1>

<ul>
<% @cart.each do |gig| %>
    <li><%= gig.title %> | <%= number_to_currency(gig.cost / 100.0) %></li>
    <% end %>
</ul>
</div>

<div class="cartpay">
<%= link_to "Pay for cart", "", id: "stripe" %>
</div>

<%= javascript_pack_tag "stripe" %>